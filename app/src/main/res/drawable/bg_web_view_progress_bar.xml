<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient android:color="@color/app_white" />
        </shape>
    </item>



    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:centerColor="@color/app_theme_color"
                    android:endColor="@color/app_theme_color"
                    android:startColor="@color/app_theme_color" />
            </shape>
        </clip>
    </item>

</layer-list>